{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": ["error", { "allow": ["_x1", "_x2", "_y1", "_y2"] }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [1, { "ignorePureComponents": true }]
  },
  "globals": {
    "document": true,
    "window": true
  }
}
